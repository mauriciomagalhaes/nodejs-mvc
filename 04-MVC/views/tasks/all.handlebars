<h1>Ver Todas as tarefas</h1>
<ul class="task-list">
  {{#each tasks}}
    <li>
        <a class="title" href="/">{{this.title}}</a>
        <div class="actions">
            <a href="/"><i class="bi bi-check2"></i></a>
            <a href="/"><i class="bi bi-pencil-square"></i></a>
            <a href="/"><i class="bi bi-x-lg"></i></a>
        </div>
    </li>
  {{/each}}
</ul>